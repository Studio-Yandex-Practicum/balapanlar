{
	"private": true,
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@babel/preset-env": "^7.18.10",
		"@htmlacademy/editorconfig-cli": "1.0.0",
		"autoprefixer": "^10.4.8",
		"babel-loader": "^8.2.5",
		"clean-webpack-plugin": "^4.0.0",
		"css-loader": "^6.7.1",
		"cssnano": "^5.1.13",
		"gh-pages": "^4.0.0",
		"html-webpack-plugin": "^5.5.0",
		"husky": "7.0.4",
		"lint-staged": "12.3.7",
		"mini-css-extract-plugin": "^2.6.1",
		"postcss-loader": "^7.0.1",
		"stylelint": "14.0.0",
